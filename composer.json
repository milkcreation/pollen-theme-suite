{
  "name": "pollen-solutions/theme-suite",
  "description": "Pollen Theme Suite Component",
  "homepage": "https://www.presstify.com/pollen-solutions/theme-suite/",
  "keywords": [
    "pollen-solutions",
    "components",
    "theme suite",
    "partial drivers",
    "field drivers",
    "metabox drivers"
  ],
  "time": "2021-06-24",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "pollen-solutions/support": "^1.0",
    "pollen-solutions/wp-post": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\ThemeSuite\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\ThemeSuite\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
